<resources>
	<properties>
		<property id="targetPace" type="string">10:00</property>
		<property id="targetPaceUnits" type="number">1</property>
	</properties>
	<settings>
	    <setting propertyKey="@Properties.targetPace" title="@Strings.TargetPaceTitle" prompt="@Strings.TargetPacePrompt">
	        <settingConfig type="alphaNumeric" />
	    </setting>
	    <setting propertyKey="@Properties.targetPaceUnits" title="@Strings.TargetPaceUnitsTitle">
	        <settingConfig type="list">
	            <listEntry value="1">@Strings.MinutesPerMile</listEntry>
	            <listEntry value="2">@Strings.MinutesPerKilometer</listEntry>
	        </settingConfig>
	    </setting>
	</settings>
</resources>
